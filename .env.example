# MCP MITM Mem0 - Environment Configuration
# Copy this file to .env and fill in your values

# Required: Get your API key from https://app.mem0.ai
MEM0_API_KEY=your_mem0_api_key_here

# Optional settings
DEBUG=false

# MITM proxy settings
MITM_HOST=localhost
MITM_PORT=8080

# MCP server settings
MCP_NAME=memory-service

# User identification (for memory organization)
DEFAULT_USER_ID=default_user